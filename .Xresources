URxvt.urgentOnBell: off
URxvt.visualBell: off
URxvt.iso14755: off
URxvt.iso14755_52: off
URxvt.meta8: true
URxvt.depth: 32
URxvt.iconFile: /usr/share/icons/Humanity/apps/128/utilities-terminal.svg
URxvt.metaSendsEscape: truee

URxvt.internalBorder: 0
URxvt.externalBorder: 0

URxvt.scrollBar: off
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.cursorColor:#ff25ff
URxvt.saveLines: 10000
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true

URxvt*depth:32
!URxvt*foreground:#ADADAD
URxvt*background:rgba:0000/0000/0000/eeee
!URxvt*background:rgba:0600/0600/0600/eeee
!URxvt.foreground: 15
URxvt.geometry: 128x38

!Droid Sans
URxvt.font: xft:Droid Sans Mono for Powerline:pixelsize=18:antialias=true:hinting=true
URxvt.boldFont: xft:Droid Sans Mono for Powerline:bold:pixelsize=18:antialias=true:hinting=true
URxvt.italicFont: xft:Droid Sans Mono for Powerline:italic:pixelsize=18:antialias=true:hinting=true
URxvt.boldItalicFont: xft:Droid Sans Mono for Powerline:bold:italic:pixelsize=18:antialias=true


!DejaVu Sans
!URxvt.font: xft:DejaVu Sans Mono for Powerline:pixelsize=17:antialias=true:hinting=true
!URxvt.boldFont: xft:DejaVu Sans Mono for Powerline:bold:pixelsize=17:antialias=true:hinting=true
!URxvt.italicFont: xft:DejaVu Sans Mono for Powerline:italic:pixelsize=17:antialias=true:hinting=true
!URxvt.boldItalicFont: xft:DejaVu Sans Mono for Powerline:bold:italic:pixelsize=17:antialias=true

!URxvt.letterSpace: -1

! Disable screen printing by overriding the default print pipe.
URxvt.print-pipe: "cat >/dev/null"

URxvt.perl-ext-common : default,keyboard-select,url-select,clipboard

URxvt.keysym.M-x           	: perl:keyboard-select:activate
URxvt.keysym.M-u           	: perl:url-select:select_next
URxvt.keysym.M-c           	: perl:clipboard:copy
URxvt.keysym.M-v           	: perl:clipboard:paste
URxvt.keysym.M-s           	: perl:keyboard-select:search
URxvt.keysym.M-C-v         	: perl:clipboard:paste_escaped
!URxvt.perl-ext-common     	: tabbed,default,matcher
!Urxvt.perl-lib            	: /usr/lib/urxvt/perl/
URxvt.url-select.launcher  	: luakit
URxvt.url-select.autocopy  	: true
URxvt.url-select.underline 	: true

URxvt.url-select.button:   1

!URxvt.clipboard.copycmd:  xsel --input  --clipboard
!URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt.keysym.Control-Shift-C: perl:clipboard:copy
URxvt.keysym.Control-Shift-V: perl:clipboard:paste



